{
  "version": "2.0",
  "logging": {
    "logging": {
      "logLevel": {
        "default": "Debug"
      },
      "fileLoggingMode": "always"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "watchDirectories": ["shared"]
}
